<h1>Listing handicaps</h1>

<table>
  <tr>
    <th>Score</th>
    <th>Date</th>
    <th>Golfer</th>
    <th>League</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @handicaps.each do |handicap| %>
  <tr>
    <td><%= handicap.score %></td>
    <td><%= handicap.date %></td>
    <td><%= handicap.golfer_id %></td>
    <td><%= handicap.league_id %></td>
    <td><%= link_to 'Show', handicap %></td>
    <td><%= link_to 'Edit', edit_handicap_path(handicap) %></td>
    <td><%= link_to 'Destroy', handicap, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Handicap', new_handicap_path %>
