<h1>Listing scores</h1>

<table>
  <tr>
    <th>Hole</th>
    <th>Strokes</th>
    <th>Match</th>
    <th>Golfer</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @scores.each do |score| %>
  <tr>
    <td><%= score.hole %></td>
    <td><%= score.strokes %></td>
    <td><%= score.match_id %></td>
    <td><%= score.golfer.full_name %></td>
  </tr>
<% end %>
</table>

<br />

<td><%= link_to 'Edit', edit_match_score_path(7, 21) %></td>
