<h1>Listing scores</h1>

<table>
  <tr>
    <th>Hole</th>
    <th>Strokes</th>
    <th>Match</th>
    <th>Golfer</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @scores.each do |score| %>
  <tr>
    <td><%= score.hole.number %></td>
    <td><%= score.strokes %></td>
    <td><%= score.match_id %></td>
    <td><%= score.golfer.full_name %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'Edit', round_match_edit_scores_for_match_path() %>
